# Environment variables for Universal Product Scraper

# Scraper Configuration
SCRAPER_HEADLESS=false
SCRAPER_TIMEOUT=30
SCRAPER_MIN_DELAY=2
SCRAPER_MAX_DELAY=5

# Paths
SOURCE_FILE=data/products.xlsx
OUTPUT_DIR=output
LOGS_DIR=logs

# Logging
LOG_LEVEL=INFO

# Performance
BATCH_SIZE=20
MAX_WORKERS=1

# Hebrew Processing
HEBREW_NORMALIZE=true
HEBREW_MATCH_THRESHOLD=0.8

# Development
MOCK_MODE=false
DEBUG=false